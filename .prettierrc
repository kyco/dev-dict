{
  "printWidth": 120,
  "semi": false,
  "arrowParens": "always",
  "trailingComma": "all",
  "singleQuote": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "",
    "@/types",
    "^@/(.*)$",
    "^@data/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}